{
  "name": "pika-pokedex-nextjs",
  "version": "0.1.0",
  "description": "Pokedex progressive web app with next js + bootstrap",
  "author": "Antonio Marin",
  "license": "MIT",
  "keywords": [
    "react",
    "next",
    "pwa",
    "progressive",
    "progressive web app",
    "web",
    "app",
    "react",
    "bootstrap",
    "pokedex",
    "pokemon"
  ],
  "scripts": {
    "dev": "cross-env NODE_ENV=development next",
    "build": "cross-env NODE_ENV=production next build",
    "start": "cross-env NODE_ENV=production next start",
    "test": "cross-env NODE_ENV=test nyc mocha",
    "coverage": "nyc report --reporter=text-lcov > coverage.lcov && codecov",
    "lint": "eslint --fix . --ignore-path ./.gitignore",
    "format": "prettier --write './**/*.{js,jsx,scss,md,json}' --config ./.prettierrc --ignore-path ./.gitignore"
  },
  "dependencies": {
    "bootstrap": "^4.5.2",
    "bootswatch": "^4.5.2",
    "express": "^4.17.1",
    "i18next": "^14.0.1",
    "isomorphic-unfetch": "3.0.0",
    "next": "^9.5.2",
    "next-i18next": "^6.0.2",
    "next-pwa": "^3.1.3",
    "pokedex-promise-v2": "^3.2.0",
    "react": "^16.13.1",
    "react-bootstrap": "^1.3.0",
    "react-dom": "^16.13.1",
    "sass": "^1.26.10",
    "swr": "^0.1.18"
  },
  "devDependencies": {
    "@babel/core": "^7.0.1",
    "@babel/register": "^7.0.0",
    "babel-eslint": "^10.1.0",
    "codecov": "^3.5.0",
    "cross-env": "7.0.2",
    "enzyme": "^3.11.0",
    "enzyme-adapter-react-16": "^1.15.2",
    "eslint": "^7.5.0",
    "eslint-config-airbnb": "^18.1.0",
    "eslint-config-prettier": "^6.11.0",
    "eslint-plugin-import": "^2.20.2",
    "eslint-plugin-jsx-a11y": "^6.3.1",
    "eslint-plugin-prettier": "^3.1.4",
    "eslint-plugin-react": "^7.20.3",
    "eslint-plugin-react-hooks": "^4.0.8",
    "expect.js": "^0.3.1",
    "mocha": "^7.1.1",
    "nyc": "^14.1.1",
    "prettier": "^2.0.5"
  }
}
